{
  "private": true,
  "name": "build",
  "type": "commonjs",
  "scripts": {
    "postinstall": "elm-tooling install",
    "pretest": "tsc && prettier --check . && eslint . --report-unused-disable-directives && node -r esbuild-register scripts/EnsureTestDependencies.ts",
    "test": "jest --coverage",
    "posttest": "npm run build",
    "build": "node -r esbuild-register scripts/Build.ts"
  },
  "devDependencies": {
    "@types/cross-spawn": "6.0.2",
    "@types/jest": "28.1.6",
    "@types/node": "18.6.4",
    "@types/ws": "8.5.3",
    "@typescript-eslint/eslint-plugin": "5.32.0",
    "@typescript-eslint/parser": "5.32.0",
    "elm-tooling": "1.8.0",
    "esbuild": "0.14.53",
    "esbuild-register": "3.3.3",
    "eslint": "8.21.0",
    "eslint-plugin-jest": "26.7.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "globals": "13.17.0",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.3",
    "prettier": "2.7.1",
    "ts-jest": "28.0.7",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "cross-spawn": "^7.0.3",
    "tiny-decoders": "^7.0.0",
    "ws": "^8.8.1"
  }
}
